    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "svelte": "^5.36.16",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.3",
    "three": "^0.178.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@eslint/js": "^9.31.0",
    "@playwright/test": "^1.54.1",
    "@testing-library/svelte": "^5.2.8",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
