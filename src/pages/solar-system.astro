---
// Redirect to new planetary routing structure
export const prerender = false;

if (Astro.request.method === 'GET') {
  return Astro.redirect('/planetary/solar', 301);
}
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="refresh" content="0; url=/planetary/solar" />
    <title>Redirecting...</title>
  </head>
  <body>
    <p>Redirecting to <a href="/planetary/solar">Solar System</a>...</p>
  </body>
</html>
